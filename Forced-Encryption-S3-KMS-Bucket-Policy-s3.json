{
  "Id": "Policy1568485165186",
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "Stmt1568485158764",
      "Action": [
        "s3:PutObject"
      ],
      "Effect": "Deny",
      "Resource": "arn:aws:s3:::your-bucket-name/*",
      "Condition": {
        "StringNotEquals": {
          "s3:x-amz-server-side-encryption": "aws:kms"
        }
      },
      "Principal": "*"
    }
  ]
}