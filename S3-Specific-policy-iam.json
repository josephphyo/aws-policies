{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "s3:GetBucketLocation",
                "s3:ListAllMyBuckets",
                "s3:*Object"
            ],
            "Resource": "arn:aws:s3:::sandbox-owaytrip-images"
        },
        {
            "Effect": "Allow",
            "Action": "s3:*",
            "Resource": [
                "arn:aws:s3:::sandbox-owaytrip-images",
                "arn:aws:s3:::sandbox-owaytrip-images/*"
            ]
        }
    ]
}